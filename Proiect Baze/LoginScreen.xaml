<Window x:Class="Proiect_Baze.LoginScreen"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Proiect_Baze"
        mc:Ignorable="d"
       
        WindowStartupLocation="CenterScreen"
        x:Name="LoginWindow"
        Icon="D:\PROIECT BAZE DE DATE\SiglaScoala"
        Title="Bine ati venit!" Height="600" Width="400" FontSize="14" Background="#291b2c">


    <StackPanel Margin="50">
        <Label Content="Autentificare" Foreground="#CCA969" FontSize="32" HorizontalAlignment="Center"/>        
        <Separator ></Separator>
        <Label Content="Introduceti codul" FontSize="25" HorizontalAlignment="Center" Foreground="#cca969" Margin="0 50 0 0"/>
        <PasswordBox Name="GetId"  Background="#CCA969" Margin="22 30 22 0" Foreground="Black" FontSize="22"/>
        <Button Name="LogButton" Content="Submit" Margin="26 30 26 0" Click="LogButton_Click"  Background="#CCA969" Foreground="AntiqueWhite" FontSize="24" FontStyle="Oblique" Height="58"/>

    </StackPanel>
</Window>
